    "eslint-plugin-tailwindcss": "^3.11.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.3.3",
